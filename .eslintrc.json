{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"plugins": [
		"@typescript-eslint",
		"prettier",
		"unicorn"
	],
	"extends": [
		"airbnb-typescript/base",
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:unicorn/recommended",
		"prettier",
		"plugin:prettier/recommended",
		"prettier/@typescript-eslint",
		"prettier/unicorn"
	],
	"env": {
		"es6": true,
		"node": true
	},
	"rules": {
		"no-console": 0
	}
}