{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": [
		"@typescript-eslint",
		"prettier",
		"unicorn"
	],
	"extends": [
		"airbnb-typescript",
		"airbnb/hooks",
		"react-app",
		"plugin:@typescript-eslint/recommended",
		"plugin:unicorn/recommended",
		"prettier",
		"plugin:prettier/recommended",
		"prettier/@typescript-eslint",
		"prettier/unicorn",
		"prettier/react"
	],
	"env": {
		"browser": true,
		"jest": true,
		"es6": true
	},
	"rules": {}
}